{
  "name": "ai-game-builder",
  "version": "1.0.0",
  "description": "A chat-only AI game builder that lets users create full 2D, 2.5D, or 3D games by conversation",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "ai",
    "game",
    "builder",
    "generator",
    "2d",
    "3d",
    "conversation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.8.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
