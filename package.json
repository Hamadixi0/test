{
  "name": "ai-game-builder",
  "version": "1.0.0",
  "description": "AI-powered cross-platform game builder that generates complete games from text prompts",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "server": "cd backend && npm start",
    "server:dev": "cd backend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../mobile && npm install",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "ai",
    "game-builder",
    "react-native",
    "expo",
    "cross-platform",
    "unity",
    "godot"
  ],
  "author": "AI Game Builder Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "private": true
}